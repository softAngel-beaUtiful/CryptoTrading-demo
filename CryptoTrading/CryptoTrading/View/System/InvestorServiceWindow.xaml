<Window x:Class="CryptoTrading.InvestorServiceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="保证金监控中心" Height="700" Width="1000" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded" Closed="Window_Closed">
    <Grid Name="winGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
        </Grid.RowDefinitions>
        <Button Name="btnOK" Grid.Row="1" Content="确认" Width="80" Height="40" FontSize="18" HorizontalAlignment="Center" VerticalAlignment="Center" Click="btnOK_Click"></Button>
    </Grid>
</Window>
